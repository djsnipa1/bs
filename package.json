{
  "name": "beatstar-practicer",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "sleep 10 && vite dev --host",
    "tmole": "tmole 5173 | tee output.log",
    "devall": "run-p tmole dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "preview": "vite preview --host",
    "test": "playwright test",
    "vomit": "pnpx vomitorium"
  },
  "vomitorium": {
    "scan": ".",
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules",
      ".git",
      "dist",
      "build"
    ],
    "excludeFiles": [
      "package.json",
      "package-lock.json",
      "vite.config.ts",
      "svelte.config.js",
      "playwright.config.ts"
    ],
    "extensions": [
      ".js",
      ".ts",
      ".svelte"
    ],
    "showExcluded": false,
    "showSkipped": false,
    "outputFile": "output.sick"
  },
  "devDependencies": {
    "@fontsource-variable/sofia-sans": "^5.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@neodrag/svelte": "^2.0.6",
    "@playwright/test": "^1.48.1",
    "@sveltejs/adapter-auto": "^3.3.0",
    "@sveltejs/adapter-cloudflare": "^4.7.3",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@texel/color": "^1.1.4",
    "@types/animejs": "^3.1.12",
    "@types/culori": "^2.1.1",
    "@types/eslint": "^8.56.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "animejs": "^3.2.2",
    "autoprefixer": "^10.4.20",
    "bezier-easing": "^2.1.0",
    "color-thief-ts": "github:charltoons/color-thief-ts",
    "culori": "^4.0.1",
    "daisyui": "^4.12.13",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "espree": "^10.2.0",
    "get-video-id": "^4.1.7",
    "node-vibrant": "3.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-load-config": "^6.0.1",
    "posthog-js": "^1.174.3",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "svelte": "^5.0.5",
    "svelte-check": "^4.0.5",
    "svelte-copy": "^2.0.0",
    "svelte-outside": "^0.0.3",
    "tailwindcss": "^3.4.14",
    "tailwindcss-document-cli": "^1.1.0",
    "tslib": "^2.8.0",
    "tunnelmole": "^2.2.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  }
}
